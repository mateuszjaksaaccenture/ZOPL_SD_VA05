<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog title="{i18n>CORR_NUMS} {CorrNo>/OrdId} ({CorrNo>/OrdPosNr})">
		<Table inset="true" mode="None" 
				items="{ path: 'CorrNo>/CorrNo' }"
				noDataText="{i18n>NO_CORR_NO}">
			<items>
				<ColumnListItem unread="true" counter="0">
					<cells>
						<Text text="{CorrNo>CorrNo}"></Text>
					</cells>
				</ColumnListItem>
			</items>
			<columns>
				<Column hAlign="Center" minScreenWidth="tablet" width="15%" demandPopin="true">
					<header>
						<Text text="{i18n>CORR_NO_FOR_SO}" textAlign="Left" width="100%" maxLines="0"></Text>
					</header>
				</Column>
			</columns>
		</Table>
		<beginButton>
			<Button text="{i18n>CLOSE}" press="onCloseDialogCorrNo"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>